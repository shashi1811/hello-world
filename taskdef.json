{
    "family": "hello-world",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "hello-world",
            "cpu": "256",
            "memory": "512"
			"requiresCompatibilities": [
            "FARGATE"
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
