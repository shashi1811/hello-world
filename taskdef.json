{
        "family": "wavefront-task-def",
        "networkMode": "awsvpc",
        "containerDefinitions": [
            {
                "name": "tomcat-example",
                "image": "tomcat:latest",
                "essential": true,
				"portMappings": [
					{
					  "hostPort": 8080,
					  "protocol": "tcp",
					  "containerPort": 8080
					}
				]
            }, 
			
        ],
        "requiresCompatibilities": [
            "FARGATE"
        ]
        "cpu": "256",
        "memory": "512"
}
